{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.png",
                    "/index.html",
                    "/*.css",
                    "/*.js",
                    "/fira-sans*",
                    "/Material-Icons-Baseline.*"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "api",
            "urls": ["/rest/*", "/apps/*", "/system/*", "/stats"],
            "cacheConfig": {
                "maxSize": 0,
                "maxAge": "0u",
                "strategy": "freshness"
            }
        }
    ]
}
